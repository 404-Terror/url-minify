{
  "name": "URL Minify Extension",
  "version": "0.0.1",
  "description": "URL Minify Extension for Chrome",
  "icons": {
    "128": "logo.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "manifest_version": 2,
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "http://*/*",
    "https://*/*",
    "*://*/*"
  ]
}
